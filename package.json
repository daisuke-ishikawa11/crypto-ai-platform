{
  "name": "crypto-ai-platform",
  "version": "2.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "next dev",
    "ingest:pools": "node --env-file=.env.local ./scripts/ingest-pools.mjs",
    "alerts:scan": "node --env-file=.env.local ./scripts/alerts-scan.mjs",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit --skipLibCheck",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:unit": "jest --testPathIgnorePatterns=e2e",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:coverage": "jest --coverage",
    "security:scan": "npm audit --audit-level=moderate",
    "production:check": "npm run type-check && npm run lint && npm run test",
    "analyze": "cross-env ANALYZE=true next build",
    "clean": "rimraf .next out coverage test-results playwright-report",
    "wsl2:fix": "bash scripts/wsl2-npm-complete-fix.sh",
    "wsl2:clean": "rm -rf node_modules package-lock.json && npm cache clean --force",
    "deps:check": "npm ls --depth=0",
    "deps:outdated": "npm outdated",
    "deps:audit": "npm audit --audit-level=moderate",
    "db:seed": "tsx scripts/seed-database.ts",
    "bun:install": "npx bun install",
    "bun:dev": "npx bun run dev",
    "bun:build": "npx bun run build",
    "bun:test": "npx bun test",
    "bun:type-check": "npx bun run type-check"
  },
