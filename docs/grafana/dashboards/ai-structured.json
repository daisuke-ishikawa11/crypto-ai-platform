{
  "title": "AI Structured API",
  "timezone": "browser",
  "panels": [
    {
      "type": "timeseries",
      "title": "Requests / Errors",
      "targets": [
        { "expr": "sum(rate(ai_structured_requests_total[5m]))", "legendFormat": "requests/s" },
        { "expr": "sum(rate(ai_structured_errors_total[5m]))", "legendFormat": "errors/s" }
      ]
    },
    {
      "type": "timeseries",
      "title": "p95 Latency (s)",
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(ai_structured_request_duration_seconds_bucket[5m])) by (le))", "legendFormat": "p95" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Requests by Type",
      "targets": [
        { "expr": "sum by (type) (rate(ai_structured_requests_total[5m]))", "legendFormat": "{{type}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Errors by Type",
      "targets": [
        { "expr": "sum by (type) (rate(ai_structured_errors_total[5m]))", "legendFormat": "{{type}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Errors by Reason",
      "targets": [
        { "expr": "sum by (reason) (rate(ai_structured_errors_total[5m]))", "legendFormat": "{{reason}}" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Errors by Status",
      "targets": [
        { "expr": "sum by (status) (rate(ai_structured_errors_total[5m]))", "legendFormat": "{{status}}" }
      ]
    }
  ],
  "schemaVersion": 38,
  "refresh": "10s",
  "version": 1
}
